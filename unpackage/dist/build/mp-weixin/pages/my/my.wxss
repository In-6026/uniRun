.my{width:calc(100% - 10px);height:auto;margin:0 auto}.my .setIcon{width:24px;height:24px;margin-left:calc(100% - 24px)}.my .profile{border-radius:3px;width:100%;height:150px;background:#439947}.my .profile .name-id{width:100%;height:50px;line-height:50px;display:flex;align-items:bottom;margin-left:20px}.my .profile .name-id image{width:20px;height:20px;margin-left:20px}.my .profile .name-id .name{color:#fff;font-size:18px;margin-left:10px}.my .profile .id-icon{display:flex;height:30px}.my .profile .id-icon .id{color:#fff;font-size:12px;margin-left:20px}.my .profile .profiletxt{color:#fff;height:40px;width:calc(100% - 40px);margin-left:20px;display:flex}.my .profile .profiletxt view{width:50%;height:100%;font-size:13px}.my .nav{width:100%;height:auto;display:flex;justify-content:space-around;flex-wrap:wrap;margin-top:10px}.my .nav view{width:calc(50% - 1px);height:60px;border-radius:3px;margin-top:2px;background:#f8f8f8}.my .nav view text{display:block;padding-left:10px}.my .nav view text:nth-child(1){height:34px;line-height:34px;font-size:16px;font-weight:bolder}.my .nav view text:nth-child(2){font-size:13px;color:grey}