.map{position:fixed;top:0;left:0;width:100%;height:calc(100% - 150px)}.switch-case{position:fixed;left:0;bottom:0;width:100%;height:150px;background:#fff}.switch-case .has-run{position:absolute;left:0;top:0;padding:20px;font-size:25px;font-weight:bolder}.switch-case .switch{position:absolute;right:20px;top:0;width:70px;height:100%;text-align:center}.switch-case .switch ._i{display:block;margin-top:-35px;width:70px;height:70px;border-radius:100%;background:#439947}.switch-case .switch ._i .swt-icon{width:40px;height:40px;margin:15px}.switch-case .switch .time{margin-top:10px}